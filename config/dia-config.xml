<?xml version="1.0"?>
<dia_config>
	<site>bvsms</site>
	<bvs_url>http://bvsms.teste.bvs.br/</bvs_url>
	<about_bvs_url>http://bvsms.teste.bvs.br/</about_bvs_url>
	<home_url>http://bvsms.teste.bvs.br/</home_url>
	<search_server>srv.homolog.bvsalud.org</search_server>
	<photocopy_url>http://scad.bvs.br/php/index.php?identificado=1</photocopy_url>
	<documents_per_page>10</documents_per_page>
	<default_lang>pt</default_lang>
	<log_dir>../../bases/logs/bvsms/</log_dir>
	<environment>teste</environment>

	<search_collection_list>
		<collection>
			<name>main</name>
			<cluster_items_limit>20</cluster_items_limit>
			<set></set>
			<filter></filter>
			<index_list>
				<index>
					<name>ALL</name>
					<prefix></prefix>
				</index>
				<index>
					<name>TI</name>
					<prefix>ti</prefix>
				</index>
				<index>
					<name>AU</name>
					<prefix>au</prefix>
				</index>
				<index>
					<name>MH</name>
					<prefix>mh</prefix>
				</index>
				<index>
					<name>PT</name>
					<prefix>pt</prefix>
				</index>

			</index_list>
			<where_list>
				<where>
					<name>ALL</name>
					<filter></filter>
				</where>
				<where>
					<name>INSTITUCIONAL</name>
					<filter>db:("ms" OR "video" OR "cartaz" OR "txtc")</filter>
				</where>
				<where>
					<level>2</level>
					<name>MS</name>
					<filter>db:"ms"</filter>
				</where>
				<where>
					<level>2</level>
					<name>VIDEO</name>
					<filter>db:"video"</filter>
				</where>
				<where>
