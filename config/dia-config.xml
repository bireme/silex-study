					<name>TXTC</name>
					<filter>db:"txtc"</filter>
				</where>
				<where>
					<name>TEMATICAS</name>
					<filter>db:("atac" OR "ataes" OR "atct" OR "atcc" OR "atda" OR "ath" OR "atpcs" OR "atspn" OR "atst" OR "atsm" OR "atss" OR "atvs" OR "inca" OR "ans" OR "crphf" OR "hgb" OR "prefsp" OR "funasa" OR "sespa")</filter>
				</where>				
				<where>
					<level>2</level>
					<name>ATAC</name>
					<filter>db:"atac"</filter>
