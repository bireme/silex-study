			<sort_list>
				 <sort>
					 <name>RELEVANCE</name>
					 <value>weight desc,score desc</value>
				 </sort>
				 <sort>
				 	 <default_for_empty_query>1</default_for_empty_query>
					 <name>YEAR_DESC</name>
					 <value>da desc</value>
				 </sort>
				 <sort>
					 <name>YEAR_ASC</name>
					 <value>da asc</value>
				 </sort>
				 <sort>
					 <name>SJR</name>
